{
  "manifest_version": 2,

  "name": "good morning & night",
  "description": "朝と夜の作業まとめ",
  "version": "1.0.0",

  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "open"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
},
  "permissions": [
    "activeTab",
    "storage",
    "tabs","http://*/*"
  ]
}