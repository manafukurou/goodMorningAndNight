{
  "manifest_version": 3,

  "name": "Tabmato",
  "description": "設定したタブをまとめて開く【Tabmato】",
  "version": "2.1.0",

  "action": {
    "default_popup": "popup.html",
    "default_title": "open"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
},
  "permissions": [
    "tabGroups",
    "tabs"
  ]
}